<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.zk.mybatisplus.mapper.TTenantRoleMapper">
    <resultMap id="BaseResultMap" type="com.zk.mybatisplus.model.TTenantRole">
        <id column="id" property="id" jdbcType="INTEGER" />
        <result property="tenantAccounts" column="tenant_accounts" jdbcType="VARCHAR"/>
        <result property="roleName" column="role_name" jdbcType="VARCHAR"/>
        <result property="roleCode" column="role_code" jdbcType="VARCHAR"/>
        <result property="valid" column="valid" jdbcType="VARCHAR"/>
        <result property="createTime" column="create_time" jdbcType="TIMESTAMP"/>
        <result property="updateTime" column="update_time" jdbcType="TIMESTAMP"/>
    </resultMap>

    <sql id="Base_Column_List" >
		id,tenant_accounts,role_name,role_code,valid,create_time,update_time
	</sql>

    <select id="findById" resultMap="BaseResultMap" parameterType="java.lang.Integer"   >
        select
        <include refid="Base_Column_List" />
        from t_tenant_role
        where id = #{id}
    </select>

    <select id="findAll"  resultMap="BaseResultMap" resultType="java.util.List" >
        select
        <include refid="Base_Column_List" />
        from t_tenant_role

    </select>
</mapper>